<div class="container">
    <h3>ジャンル編集</h3>
    <div class="row">
        <div class="col-5">
            <table>
             <%= form_with medel:@genre, local:true do |f| %>
              <td>
                <div class="form-group">
                    <%= f.label :name, "ジャンル名" %>
                <%= f.text_field :name, clas: 'form-control' %>
                </div>
              </td>
              <td>
                <div class="form-group">
                <%= f.submit "変更を保存", class: 'btn btn-success' %>
                </div>
              </td>
             <%end%>
            </table>
        </div>
    </div>
</div>
